<!DOCTYPE html>
<html lang="pt">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- ICON -->
    <link rel="shortcut icon" href="/img/logo/icon.png" type="image/png">

    <!-- TITLE -->
    <title>A Saboeira - Turismo Rural</title>

    <!-- BOOTSTRAP v5 -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>

    <!-- FONTAWESOME -->
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.15.4/css/all.css" integrity="sha384-DyZ88mC6Up2uqS4h/KRgHuoeGwBcD4Ng9SiP4dIRy0EXTlnuz47vAwmeGwVChigm" crossorigin="anonymous">

    <!-- JQUERY -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>

    <!-- HOVER.CSS -->

    <!-- FANCY BOX -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/fancybox/3.2.5/jquery.fancybox.min.js"></script>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/fancybox/3.2.5/jquery.fancybox.min.css" />

    <!-- GALLERIES -->
    <link rel="stylesheet" href="/css/gallery.css">
    <script src="/js/gallery.js"></script>
    <script src="/js/rooms.js"></script>

    <!-- CUSTOM STYLES -->
    <link rel="stylesheet" href="/css/gallery.css">
    <link rel="stylesheet" href="/css/style.css">

</head>



<body>

    <!-- LOADER -->
    <%- include('loader.ejs') %>
        <!-- LOADER -->


        <!-- LANDING -->
        <div class="teaser">

            <!-- NAVBAR -->
            <div class="container position-absolute start-50 translate-middle-x nav py-4">
                <div class="row w-100">

                    <!-- TITLE -->
                    <div class="col-3">
                        <div class="navbar-title">
                            <a href="#">
                                <h3>A Saboeira</h3>
                                <h4>Turismo Rural</h4>
                            </a>
                        </div>
                    </div>
                    <!-- TITLE -->


                    <!-- SPACER -->
                    <div class="col"></div>
                    <!-- SPACER -->


                    <!-- LINKS -->
                    <div class="col">
                        <div class="row d-flex align-content-center">

                            <div class="col">
                                <a href="#galeria" class="px-3">
                                    <h5>GALERIA</h5>
                                </a>
                            </div>
                            <div class="col">
                                <a href="#quartos" class="px-3">
                                    <h5>QUARTOS</h5>
                                </a>
                            </div>
                            <div class="col">
                                <a href="#contactos" class="px-3">
                                    <h5>CONTACTOS</h5>
                                </a>
                            </div>
                            <div class="col">
                                <a href="#sobre" class="px-3">
                                    <h5>SOBRE</h5>
                                </a>
                            </div>

                        </div>
                    </div>
                    <!-- LINKS -->

                </div>
            </div>
            <!-- NAVBAR -->


            <!-- VIDEO -->
            <div class="overlay"></div>
            <video src="teaser.mp4" autoplay muted loop></video>
            <!-- VIDEO -->


        </div>
        <!-- LANDING -->






        <!-- GALLERY -->
        <%- include('gallery.ejs') %>
            <!-- GALLERY -->



            <!-- PARALLAX -->
            <div class="container parallax">
                <img class="prl thumbnail" src="/img/background/1.jpg" alt="image">
            </div>
            <!-- PARALLAX -->


            <!-- ROOMS GALLERY -->
            <%- include('rooms.ejs') %>
                <!-- ROOMS GALLERY -->




                <!-- CONTACTS  -->
                <%- include('contacts.ejs') %>
                    <!-- CONTACTS  -->


                    <div class="container parallax">
                        <img class="prl thumbnail" src="/img/background/2.jpg" alt="image">
                    </div>


                    <!-- SOBRE -->
                    <%- include('about.ejs') %>
                        <!-- SOBRE -->





                        <!-- MAP -->
                        <%- include('map.ejs') %>
                            <!-- MAP -->





                            <!-- PARALLAX.JS -->
                            <script src="https://cdn.jsdelivr.net/npm/simple-parallax-js@5.5.1/dist/simpleParallax.min.js"></script>
                            <script src="/js/parallax.js"></script>


                            <!-- LOADER -->
                            <script src="/js/loader.js"></script>
</body>

</html>